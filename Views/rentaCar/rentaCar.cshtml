@model List<Selected_Car_Info>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>cars</title>
    <link rel="stylesheet" href="/css/rentaCar.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body>
    <div class="card" style="width: 18rem;">
        <img src="/images/R.jpg" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">Card title</h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        </div>
        <ul class="list-group list-group-flush">
            @foreach (var car in Model)
            {
                <li class="list-group-item">@car.Car_Model</li>
                <li class="list-group-item">@car.Doors_Number</li>
                <li class="list-group-item">@car.Seats</li>
                <li class="list-group-item">@car.Lugage</li>
                <li class="list-group-item">@car.Transmission</li>
                <li class="list-group-item">@car.Price</li>
            }
        </ul>
       @* <div class="card-body">
            <a href="#" class="card-link">Card link</a>
            <a href="#" class="card-link">Another link</a>
        </div>*@
    </div>
</body>
</html>